zend_extension=opcache
opcache.enable={{ env.OPCACHE_ENABLED | default(1) }}
opcache.memory_consumption={{ env.OPCACHE_MEMORY_CONSUMPTION | default(128) }}
opcache.interned_strings_buffer={{ env.OPCACHE_INTERNED_STRINGS_BUFFER | default(8) }}
opcache.max_accelerated_files={{ env.OPCACHE_MAX_ACCELERATED_FILES | default(4000) }}
opcache.huge_code_pages={{ env.OPCACHE_HUGE_CODE_PAGES | default(1) }}
opcache.validate_timestamps={{ env.OPCACHE_VALIDATE_TIMESTAMPS | default(0) }}
